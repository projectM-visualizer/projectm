comp_1=`float3 ret1, neu;
comp_2=`float2 rs,rs2,uv1;
comp_3=`shader_body
comp_4=`{
comp_5=`float2 uv2;
comp_6=`float ang2, c, s;
comp_7=`float dist = 1;
comp_8=`float inten = 1;
comp_9=`ret1 = 0;
comp_10=`int anz = 2;//# layers
comp_11=`float n = 0;
comp_12=`uv -= .5;
comp_13=`uv *= aspect.xy*1.9;    
comp_14=`while (n <= anz) {
comp_15=`    uv2=uv;
comp_16=`    if      (n%3==0) dist = 1-frac(1.0/anz*n+q10);
comp_17=`    else if (n%3==1) dist = 1-frac(1.0/anz*n+q9);
comp_18=`    else             dist = 1-frac(1.0/anz*n+q8);
comp_19=`    inten =  dist*(1-dist)*3;
comp_20=`    float2 uv3 = 0;
comp_21=`    if      (n%3==0) uv3 =  frac(1.5*uv2*dist+q8+float2(q9+dist/3,0));
comp_22=`    else if (n%3==1) uv3 =  frac(1.5*uv2*dist+q9+float2(q8+dist/3,0));
comp_23=`    else             uv3 =  frac(1.5*uv2*dist+q10+float2(q10+dist/3,0));
comp_24=`    uv3=abs(frac(uv3+.5)-.5) ;
comp_25=`    neu = GetPixel(uv3) -.7*GetBlur3(uv3*1.05);
comp_26=`    ret1 = max(ret1,neu*inten);
comp_27=`    n++;
comp_28=`}
comp_29=`rs2 = .3*sin(uv*4+q15) - lum(ret1);//sub lum(ret1) pretty much is the light distortion effect
comp_30=`float dots = (.05/length(rs2))*q26*.5;
comp_31=`ret = (q1+.5)*ret1 + dots*float3 (q8,q9,q10) ;
comp_32=`}
