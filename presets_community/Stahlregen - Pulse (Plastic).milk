per_frame_2=wave_g = .85+.15*sin(2*time);
per_frame_3=dbass = .5*dbass+.5*bass;
per_frame_4=basst = basst+.1*dbass;
per_frame_5=q1 = .15*time+.85*basst;
per_frame_6=warp = 0;
per_pixel_1=box = above(rad,0.3+.3*tan(4*q1))*below(rad,1+.3*tan(4*q1));
per_pixel_2=box2 = above(rad,0.3+.3*tan(4*q1+1.57))*below(rad,1+.3*tan(4*q1+1.57));
