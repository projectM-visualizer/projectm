comp_30=`                                           0)*0.5
comp_31=`                       )*texsize.xyy*texsize_noisevol_hq.zww
comp_32=`                      ).xyz*
comp_33=`                      float3(1,1,0)*0.05 + 
comp_34=`                      time*float3(0,0,1)*0.2 
comp_35=`                     );
comp_36=`ret *= (noise2)*1; 
