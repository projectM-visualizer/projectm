warp_16=`                     );
warp_17=`if(ret.x> q21*q13         && ret.x<= q24*q11        )ret.y += (noise9).x*.5; 
warp_18=`if(ret.y> q22*q11         && ret.y<= q25*q12        )ret.z += (noise9).y*.5; 
warp_19=`if(ret.z> q23*q12         && ret.z<= q26*q13        )ret.x += (noise9).z*.5; 
warp_20=`      ret.xyz -= 0.004;
warp_21=`
warp_22=`      // add noise:
warp_23=`      float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw * 0.4 + rand_frame.xy;
warp_24=`      ret.xyz += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)*0.18;
warp_25=`
warp_26=`      // desaturate over time, to keep the globs white
warp_27=`      ret.xyz = lerp(ret.xyz, lum(ret.xyz), 0.2);
warp_28=`}
