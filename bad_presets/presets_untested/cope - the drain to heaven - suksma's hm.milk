warp_21=`    
warp_22=`
warp_23=`}
comp_1=`shader_body
comp_2=`{
comp_3=`    ret = tex2D(sampler_main, uv).xyz;
comp_4=`    ret += GetBlur2(uv);
comp_5=`    ret = ret;
comp_6=`    ret *= 1.00; //gamma
comp_7=`// ***** Flexis rotating caleidoscope code *****
comp_8=`float2 cntr = float2(q13,q14); float sin = q11; float cos = q12; float scale = q15;
comp_9=`float2 uv_r = (uv-cntr)*aspect.xy;
comp_10=`       uv_r = scale*float2( cos*uv_r.x - sin*uv_r.y, sin*uv_r.x + cos*uv_r.y); // rotation
comp_11=`       uv_r = cntr + uv_r*aspect.zw;
comp_12=`       uv_r = 1.0 - abs( frac( uv_r * 0.5 ) * 2.0 - 1.0 ); // mirror
comp_13=`sin = q17; cos = q18; scale = q16;
comp_14=`float2 uv_rr = (uv_r-cntr);
comp_15=`       uv_rr = scale*float2( cos*uv_rr.x - sin*uv_rr.y, sin*uv_rr.x + cos*uv_rr.y); // counter-rotation
comp_16=`       uv_rr = cntr + (uv_rr);
comp_17=`float t = q22;
comp_18=`float2 c = ((uv_rr-0.5));
comp_19=`float1 ang_hq = atan2(c.x,c.y);
comp_20=`float2 spiral = float2(ang_hq*q21*1.5,0.666*log(length(c))-ang_hq*q21+t);
comp_21=`spiral = 0.5 + (0.5 - abs( frac( spiral * 0.5 ) * 2.0 - 1.0 ))*float2(0.96,1.02); // mirror
comp_22=`ret *= GetPixel(saturate(spiral));
comp_23=`}
