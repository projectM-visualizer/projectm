comp_1=`
comp_2=`float2 factorA, factorB, product;
comp_3=`float2 numerator, denominator, fraction;
comp_4=`
comp_5=`shader_body
comp_6=`{
comp_7=`float2 uvo = uv;
comp_8=`
comp_9=`//Roaming Zoom effect;
comp_10=`
comp_11=`//first create a value which moves uv, doesn't need to be too precise
comp_12=`half2 zoom_mod=uv-q7;
comp_13=`
comp_14=`//then one to multiply by!
comp_15=`half1 zoom=(q6);
comp_16=`zoom_mod*=zoom.xx;
comp_17=`
comp_18=`uv=zoom_mod;
comp_19=`
comp_20=`//*** Flexis Moebius transformation code
comp_21=`
comp_22=`uv = (uv-0.5)*aspect.xy*2;
comp_23=`factorA = uv;
comp_24=`factorB = float2(q21,q22);
comp_25=`product = float2( factorA.x*factorB.x - factorA.y*factorB.y,   // the products real part
comp_26=`                  factorA.x*factorB.y + factorA.y*factorB.x);  // the products imaginary part
comp_27=`
comp_28=`numerator = product + float2(q23,q24);
comp_29=`
comp_30=`factorA = uv;
comp_31=`factorB = float2(q25,q26);
comp_32=`product = float2( factorA.x*factorB.x - factorA.y*factorB.y,   // the products real part
comp_33=`                  factorA.x*factorB.y + factorA.y*factorB.x);  // the products imaginary part
comp_34=`
comp_35=`
comp_36=`denominator = product + float2(q27,q28);
comp_37=`
comp_38=`fraction = float2( numerator.x*denominator.x + numerator.y*denominator.y,   // the fractions real part
comp_39=`                   numerator.y*denominator.x - numerator.x*denominator.y)/  // the fractions imaginary part
comp_40=`                   (denominator.x*denominator.x + denominator.y*denominator.y);
comp_41=`
comp_42=`uv = fraction*1;
comp_43=`
comp_44=`// ***
comp_45=`
comp_46=`// *** Flexis logarithmic spiral code
comp_47=`
comp_48=`float t = 1.5*time; // set velocity here
comp_49=`float2 c = (uv-0.5)*aspect.wz;
comp_50=`float1 ang_hq = atan2(c.x,c.y);
comp_51=`// q11 = 1/asin(1); from the init section
comp_52=`float2 spiral = float2(ang_hq*q11*7+t,aspect.w*log(length(c))+ang_hq*q11-t);
comp_53=`spiral = 0.5 + (0.5 - abs( frac( spiral * 0.5 ) * 2.0 - 1 ))*0.5; // mirror
comp_54=`uv = spiral.yx;
comp_55=`
comp_56=`// ***
comp_57=`
comp_58=`//You can change the speed and direction of rotation by changing this variable
comp_59=`float rot=.7+.0006*frac(uv);
comp_60=`
comp_61=`//This variable defines the center of rotation (cx, cy)
comp_62=`float2 cxy=float2(.5,.5);
comp_63=`
comp_64=`float2 xymod=uv-cxy;
comp_65=`float2 xy1=0;
comp_66=`
comp_67=`//Rotate xy plane
comp_68=`xy1.x=sin(time*rot)*xymod.x-cos(time*rot)*xymod.y;
comp_69=`xy1.y=cos(time*rot)*xymod.x+sin(time*rot)*xymod.y;
comp_70=`
comp_71=`float2 rt=(xy1);
comp_72=`float2 rmod=rt-.5;
comp_73=`
comp_74=`uv=rmod;
comp_75=`
comp_76=`float2 uvm=0;
comp_77=`
comp_78=`uvm.x=uv.x-.07*tex2D(sampler_noise_hq, uv.x*.8).x;
comp_79=`uvm.y=uv.y-.07*tex2D(sampler_noise_hq, uv.y*.8).x;
comp_80=`
comp_81=`float3 blur = GetBlur1(uvm);
comp_82=`float3 sharp = tex2D(sampler_main, uvm);
comp_83=`float3 fix = GetPixel(frac(uvm));
comp_84=`
comp_85=`float2 mod = 4*float2(1*(fix.x)+cos(uvm.x-roam_sin.y*2-2*q31), 1*(fix.x)-sin(uvm.y+roam_cos.x*2-2*q32));
comp_86=`
comp_87=`float3 fret = (10*fix-blur*rad)-mod.xxx;
comp_88=`float3 inv = 1-fret;
comp_89=`
comp_90=`float melt = dot(fret, inv);
comp_91=`
comp_92=`ret=tex2D(sampler_main, uv.xy)*inv+.55*fret;
comp_93=`float3 modret=lerp(ret, inv, 1.5*saturate(melt));
comp_94=`
comp_95=`float3 blur1 = GetBlur1(uvm);
comp_96=`float3 sharp1 = tex2D(sampler_main, uvm);
comp_97=`//float3 fix1 = GetPixel(frac(uvm));
comp_98=`
comp_99=`float2 mod1 = 4*float2(1*(fix.z)+cos(uvm.x-roam_sin.y*2-2*q31), 1*(fix.z)-sin(uvm.y+roam_cos.x*2-2*q32));
comp_100=`
comp_101=`float3 fret1 = (10*fix-blur1*rad)-mod1.xxx;
comp_102=`float3 inv1 = 1-fret1;
comp_103=`
comp_104=`float melt1 = dot(fret1, inv1);
comp_105=`
comp_106=`ret=tex2D(sampler_main, uv)+inv1+.5*fret1;
comp_107=`float3 modret1=lerp(ret, inv1, 1.5*saturate(melt1));
comp_108=`
comp_109=`ret=lerp(saturate(modret*modret*.5), modret*modret1, .5);
comp_110=`}
