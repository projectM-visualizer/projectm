comp_44=`    uv2.y =  spiral.x*s + spiral.y*c;
comp_45=`    dist = 1-frac(1.0/anz*n+q9); //evtl sqrt !
comp_46=`    inten =  sqrt(dist)*(1-dist)*2;
comp_47=`    float2 uv3 = 3*uv2*dist*(1+q5*spiral.x) + 0.5 +float2(q10*n,q12) ;
comp_48=`    float3 cmul = float3(dist,1-dist,1-(dist-.5)*(dist-.5)*4);
