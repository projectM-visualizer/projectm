comp_27=`                    factorA.x*factorB.y + factorA.y*factorB.x);  // the products imaginary part
comp_28=`  denominator = product + float2(q20,q19);
comp_29=`  fraction = float2( numerator.x*denominator.x + numerator.y*denominator.y,   // the fractions real part
comp_30=`                     numerator.y*denominator.x - numerator.x*denominator.y)/  // the fractions imaginary part
comp_31=`                     (denominator.x*denominator.x + denominator.y*denominator.y);
comp_32=`  uv = fraction*0.5;
