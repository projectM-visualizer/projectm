per_frame_init_1=rg = 0;
per_frame_1=wave_r = wave_r + 0.350*( 0.60*sin(0.980*time) + 0.40*sin(1.047*time) );
per_frame_2=wave_g = wave_g + 0.350*( 0.60*sin(0.835*time) + 0.40*sin(1.081*time) );
per_frame_3=wave_b = wave_b + 0.350*( 0.60*sin(0.814*time) + 0.40*sin(1.011*time) );
per_frame_4=q1 = (cx*2-1) + 0.52*( 0.60*sin(0.374*time) + 0.40*sin(0.294*time) );
per_frame_5=q2 = (cy*2-1) + 0.52*( 0.60*sin(0.393*time) + 0.40*sin(0.223*time) );
per_frame_6=q3 = (cx*2-1) + 0.52*( 0.60*sin(0.174*-time) + 0.40*sin(0.364*time) );
per_frame_7=q4 = (cy*2-1) + 0.52*( 0.60*sin(0.234*time) + 0.40*sin(0.271*-time) );
per_frame_8=decay = decay - 0.01*equal(frame%5,0);
per_frame_9=
per_frame_10=// this is a great way to respond to beats:
per_frame_11=// once you get one, let it decay at a constant rate!!
per_frame_12=rg = max(rg*0.95, 0.3 + 0.5*min(2,max(0,mid_att-1)*1.3));
per_frame_13=q9 = rg;
per_pixel_1=du = x*2-1 - q1;
per_pixel_2=dv = y*2-1 - q2;
per_pixel_3=dist = sqrt(du*du+dv*dv);
per_pixel_4=ang2 = atan2(du,dv);
per_pixel_5=mult = 0.008/(dist+0.4);
per_pixel_6=dx = mult*sin(ang2-1.5);
per_pixel_7=dy = mult*cos(ang2-1.5);
per_pixel_8=du = x*2-1 - q3;
per_pixel_9=dv = y*2-1 - q4;
per_pixel_10=dist = sqrt(du*du+dv*dv);
per_pixel_11=ang2 = atan2(du,dv);
per_pixel_12=mult = 0.008/(dist+0.4);
per_pixel_13=dx = dx + mult*sin(ang2+1.5);
per_pixel_14=dy = dy + mult*cos(ang2+1.5);
warp_1=`shader_body
warp_2=`{
warp_3=`    // q9 stores a super smooth bass-driven control signal from the per-frame equations
warp_4=`    // here we lerp between unwarped & warped UV's based on it, 
warp_5=`    // effectively throttling the amount of motion!
warp_6=`    float t = q9;
warp_7=`    uv = lerp(uv_orig, uv, t);
warp_8=`
warp_9=`    // sample previous frame
warp_10=`    ret.xyz = tex2D( sampler_main, uv ).xyz;
warp_11=`
warp_12=`      // take the difference between the crisp and blurred images,
warp_13=`      // then add it back into the image.  Creates spots and stripes over time.
warp_14=`      ret.xyz += (ret - GetBlur1(uv))*0.3;
warp_15=`      ret.xyz *= 0.9;
warp_16=`
warp_17=`      // add noise:
warp_18=`      float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw * 0.4 + rand_frame.xy;
warp_19=`      ret.xyz += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)/256.0 * 122 * saturate(treb_att-1);
warp_20=`
warp_21=`      // desaturate over time, to keep the globs white
warp_22=`      ret.xyz = lerp(ret.xyz, lum(ret.xyz), 0.2);
warp_23=`}
comp_1=`shader_body
comp_2=`{
comp_3=`    ret = tex2D(sampler_main, uv).xyz;
comp_4=`    ret = lum(ret);
comp_5=`    //ret = lerp(float3(0.3,0.5,0.2)*0.9, 1.05*float3(0.4,0.6,0.3), ret.x);
comp_6=`    ret = 0.3 + 0.4*ret;
comp_7=`    ret *= 1.00;    
comp_8=`}
