per_frame_init_1=batr=bass_att+treb_att;
per_frame_init_2=natr=batr;
per_frame_init_3=oldth=1;
per_frame_1=//1st beat code
per_frame_2=oldth=thresh;
per_frame_3=thresh = above(bass_att,thresh)*2+(1-above(bass_att,thresh))*((thresh-1.3)*0.96+1.3);
per_frame_4=thcha=abs(oldth-thresh);
per_frame_5=thaddict=(thaddict+thcha)/2;
per_frame_6=thspik=above(thcha,thaddict);
per_frame_7=//dx_r = equal(thresh,2)*0.015*sin(5*time)+(1-equal(thresh,2))*dx_r;
per_frame_8=//dy_r = equal(thresh,2)*0.015*sin(6*time)+(1-equal(thresh,2))*dy_r;
per_frame_9=//2nd beat code
per_frame_10=natr=bass_att+thresh*treb_att;
per_frame_11=cha=abs(batr-natr);
per_frame_12=spik=above(abs(cha-addict),addict);
per_frame_13=q1=spik;
per_frame_14=addict=(addict+cha)/2;
per_frame_15=//beatGEN:
per_frame_16=spik=above((below(spik,0)*thspik+spik)+thspik,0);
per_frame_17=q2=spik;
per_frame_18=pt=if(spik,time,pt);
per_frame_19=spik=if(below(time-pt,0.5),spik,1);
per_frame_20=q3=spik;
per_frame_21=q4=thresh;
per_frame_22=q5=natr;
per_frame_23=//effects here
per_frame_24=zoom=zoom+if((above(bass*2,treb)-1),-1,1)*above(cha*thresh,2)*0.2;
per_frame_25=//rot=rot+if((above(bass*2,treb)-1),-1,1)*above(cha*thresh,2)*0.1;
per_frame_26=//zoomexp=2/zoom;
per_frame_27=dx=cos(time*3+10*spik)*0.015;
per_frame_28=dy=sin(time*1.5+10*spik)*0.015;
per_frame_29=mv_r=log(thresh)*spik;
per_frame_30=mv_b=abs(spik*cha-1);
per_frame_31=mv_g=sin(time+thresh);
per_frame_32=mv_a=spik*.75;
per_frame_33=ob_r=mv_a;
per_frame_34=ob_g=mv_b;
per_frame_35=ob_b=mv_g;
per_frame_36=monitor=spik;
per_frame_37=wave_r = wave_r + 0.35*sin(3*time) + 0.15*sin(2.5*time);
per_frame_38=wave_g = wave_g + 0.35*sin(2.7*time) + 0.15*sin(2.11*time);
per_frame_39=wave_b = wave_b + 0.35*sin(2.84*time) + 0.15*sin(2.3*time);
per_frame_40=wave_mystery=wave_mystery*(addict);
per_frame_41=//end effects
per_frame_42=
per_frame_43=cog = ocog+0.005*(bass+bass_att+(bass*bass_att)-2);
per_frame_44=ocog = below(cog,30000)*cog;
per_frame_45=
per_frame_46=//this must be at the END:
per_frame_47=oldth=thresh;
per_frame_48=batr=natr;
per_frame_49=
per_frame_50=//Hai guiz, you found the secret message. What are you gonna do now?
per_pixel_1=
per_pixel_2=w= -time*0.3; // direction (0: left; asin(1): top; (+/-)2*asin(1): right; -asin(1): bottom)
per_pixel_3=
per_pixel_4=X2 = x-0.5;
per_pixel_5=Y2 = y-0.5;
per_pixel_6=
per_pixel_7=X1 = sin(w)*X2 -cos(w)*Y2; // these 2 lines first rotate the x/y pane
per_pixel_8=Y1 = cos(w)*X2 +sin(w)*Y2;
per_pixel_9=
per_pixel_10=x = X1; // so you can use x and y as usually below
per_pixel_11=y = Y1;
per_pixel_12=//beat vars
per_pixel_13=speedspik=q1;
per_pixel_14=spik=q2;
per_pixel_15=slowspik=q3;
per_pixel_16=thresh=q4;
per_pixel_17=natr=q5;
per_pixel_18=//end
per_pixel_19=  //Tiles- 
per_pixel_20=  //movement first:
per_pixel_21=  dxm=.01*sin(time*.41);
per_pixel_22=  dym=.01*sin(time*.36);
per_pixel_23=  //Now the tiles themselves:
per_pixel_24=  dx1=dxm+.01*sin(x*80+sin(time))+.005*sin(q1*(y*sin(time*.94)));
per_pixel_25=  dy1=dym+.01*sin(y*80+sin(time))+.005*sin(q1*(x*sin(time*.97)));
per_pixel_26=  //end tiles
per_pixel_27=//added switching effects - noise & tiles.
per_pixel_28=dx = dx+equal(slowspik,0)*(sin(-w)*dx1 - cos(-w)*dy1); // and finally the dx/dy pane is rotated back to match the x/y pane
per_pixel_29=dy = dy+equal(slowspik,0)*(cos(-w)*dx1 + sin(-w)*dy1);
per_pixel_30=//noise
per_pixel_31=zoom=zoom+sin(int(rand(1000)))*0.1*speedspik;
per_pixel_32=//The stretchy part...
per_pixel_33=sx=sx+.25*y*sin(x*10);
per_pixel_34=sy=sy+.25*x*sin(y*10);
