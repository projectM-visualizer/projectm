per_frame_1=PI = 3.14159265;
per_frame_2=NUM_FRANES = 30;
per_frame_3=
per_frame_4=vol = pow(bass+mid+treb,2);
per_frame_5=basssum = vol;
per_frame_6=
per_frame_7=stickybit = time%2;
per_frame_8=
per_frame_9=//avg vol
per_frame_10=//buffer 1
per_frame_11=volAvg = volAvg + vol*equal(stickybit,1);
per_frame_12=//number of samples 1 or framerate
per_frame_13=sample1 = sample1 + equal(stickybit,1);
per_frame_14=//buffer 2
per_frame_15=volAvg2 = volAvg2 + vol*equal(stickybit,0);
per_frame_16=//number of samples 2
per_frame_17=sample2 = sample2 + equal(stickybit,0);
per_frame_18=
per_frame_19=//transision
per_frame_20=edge = bnot(equal(bit2,stickybit));
per_frame_21=
per_frame_22=volAvg = volAvg - volAvg*edge*stickybit;
per_frame_23=volAvg2 = volAvg2 - volAvg2*edge*equal(stickybit,0);
per_frame_24=
per_frame_25=sample1 = sample1  - sample1*edge*stickybit;
per_frame_26=sample2 = sample2  - sample2*edge*equal(stickybit,0);
per_frame_27=
per_frame_28=//test vol against avg buffer lvl
per_frame_29=diff = if(equal(stickybit,1), (basssum/(volAvg2/sample2)) , 0);
per_frame_30=diff = if(equal(stickybit,0), (basssum/(volAvg/sample1)), diff);
per_frame_31=
per_frame_32=bit2 = time%2;
per_frame_33=
per_frame_34=//only change angle after NUM_FRAMES
per_frame_35=lockFrame = if(below(lockFrame, 0)*above(diff, 4),NUM_FRANES, lockFrame);
per_frame_36=steer_angle = if(equal(lockFrame, NUM_FRANES), angle, steer_angle);
per_frame_37=
per_frame_38=angle = if(above(diff, 3)*equal(lockFrame,NUM_FRANES), cos(int(rand(10000))*0.001)*PI,angle);
per_frame_39=lockFrame = lockFrame - 1;
per_frame_40=
per_frame_41=
per_frame_42=//max to -pi - pi range
per_frame_43=steer_angle = if(below(angle - steer_angle, -PI), steer_angle - PI, steer_angle);
per_frame_44=steer_angle = if(above(angle - steer_angle, PI), steer_angle + PI, steer_angle);
per_frame_45=
per_frame_46=steer_angle = steer_angle +  (angle - steer_angle)*0.05;
per_frame_47=
per_frame_48=
per_frame_49=basstime  = basstime + bass*0.03;
per_frame_50=q1 = basstime*0.02;
per_frame_51=q2 = aspectx;
per_frame_52=q3 = cos(steer_angle);
per_frame_53=q4 = sin(steer_angle);
per_frame_54=q5 = cos(sin(basstime) + basstime)*0.05;
per_frame_55=q6 = sin(sin(basstime*1.1) + basstime*1.1)*0.05;
per_frame_56=
per_frame_57=
per_frame_58=monitor = aspectx;
per_frame_59=
per_frame_60=//angle = -1.57;
per_frame_61=
per_frame_62=q7 = sin(-angle)*basstime;
per_frame_63=q8 = cos(angle + 3.14)*basstime;
per_pixel_1=zoom = 1;
per_pixel_2=rot = 0;
warp_1=`shader_body
warp_2=`{
warp_3=`    ret = tex2D( sampler_main, uv ).xyz;
warp_4=`    ret = ret*0.8; //or try: ret -= 0.004;
warp_5=`}
comp_1=`sampler sampler_fc_plane;
comp_2=`
comp_3=`
comp_4=`shader_body
comp_5=`{
comp_6=`  //sample and rotate the plane
comp_7=`  float2x2 rot = {q3, q4, 
comp_8=`                -q4, q3};  
comp_9=`  float2 uv_adj = float2(uv.x + q5,uv.y+ 0.25 + q6)*aspect.xy;
comp_10=`  float4 plane = tex2D(sampler_fc_plane, mul(uv_adj-0.5, rot)*4 + 0.5);  
comp_11=`        
comp_12=` 
comp_13=`  //perlin noise
comp_14=`  float3 final_noise = float3(0,0,0);
comp_15=`  float3 uvx = float3(uv.x + q7, uv.y + q8, 2 + q1);  
comp_16=`  for(int i=0;i<4;i++)
comp_17=`      final_noise +=(1.0/pow(2,i))*(tex3D(sampler_noisevol_hq, uvx*pow(2,i))*2-1);
comp_18=`
comp_19=`   // Remove the sign from the noise and prep 
comp_20=`   // it for cloud rendering
comp_21=`  ret = ((final_noise+0.15)*2).yyy*0.75;
comp_22=`  
comp_23=`
comp_24=`  ret = float3(ret.x,ret.y,0.8 + 0.05*ret.x); 
comp_25=`  //fly throught the clouds
comp_26=`  float3 plane_cloud =  (ret.x  > 0.3 ? ret  :  0)*(plane.w*2 - plane.w)*0.5;
comp_27=`  ret = tex2D(sampler_main, uv) + ret*(1 - plane.w) +  plane_cloud   + plane*plane.w;
comp_28=`}
