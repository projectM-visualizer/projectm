per_pixel_3=dy = dy + if(below(y,sin(time/x)), -0.008*sin(bass), 0);
per_pixel_4=dx = dx + if(above(y,sin(time/x)), +0.008*sin(bass), 0);
per_pixel_5=dy = dy + if(above(y,sin(time/x)), +0.008*sin(basS), 0);
