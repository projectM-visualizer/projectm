per_frame_2=wave_r = -0.1;
per_frame_3=wave_g = -0.1;
per_frame_4=wave_x = wave_x + 0.25*sin(1.934*time);
per_frame_5=wave_y = wave_y + 0.25*sin(2.156*time);
per_frame_6=wave_mystery = wave_mystery + if (above(bass,0.1), +0.001 + 0.5*sin(0.01*time), -0.5);
