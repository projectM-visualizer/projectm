per_frame_18=warp = warp * (1 + 0.3*cos(time*0.284+4));
per_frame_19=zoom = zoom + 0.007*cos(time*0.317+2);
per_frame_20=vol=1+(bass+treb+mid)*.003;
per_frame_21=q2=vol;q3=vol;q11=vol;q12=vol;
