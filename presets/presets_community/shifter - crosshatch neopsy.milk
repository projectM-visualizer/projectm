per_frame_18=
per_frame_19=ra = 1/tic*.25;
per_frame_20=treb_avg = tic*(treb_avg*(1/tic - ra) + ra*treb);
per_frame_21=mid_avg = tic*(mid_avg*(1/tic - ra) + ra*mid);
per_frame_22=bass_avg = tic*(bass_avg*(1/tic - ra) + ra*bass);
per_frame_23=vav = tic*(vav*(1/tic - ra) + ra*(bass+treb+mid)*.33333);
per_frame_24=
per_frame_25=tt = tt + tic*treb;
per_frame_26=mt = mt + tic*mid;
per_frame_27=bt = bt + tic*bass;
