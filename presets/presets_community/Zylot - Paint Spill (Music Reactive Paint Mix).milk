warp_3=`    // sample previous frame
warp_4=`    float2 muv;
warp_5=`    muv.x = uv.x-((.01*cos(uv.x*(5+2*bass_att)+time))+(.01*cos(uv.y*(5+2*bass_att)+time)))*.5*treb_att; 
warp_6=`    muv.y = uv.y+((.01*sin(uv.x*(5+2*mid_att)+time))-(.01*cos(uv.y*(5+2*mid_att)+time)))*.5*bass_att;
warp_7=`    float3 blur1 = GetBlur2(muv);
warp_8=`    ret = tex2D( sampler_pw_main, muv ).xyz;
warp_9=`    ret.x = ret.x+(blur1.x)*(bass_att*.05);
warp_10=`    ret.y = ret.y+(blur1.y)*(treb_att*.05);
warp_11=`    ret.z = ret.z+(blur1.z)*(mid_att*.05); 
warp_12=`    if(ret.x > .9){
warp_13=`      ret.x = 0;
warp_14=`    }
warp_15=`    if(ret.y > .9){
warp_16=`      ret.y = 0;
warp_17=`    }
warp_18=`    if(ret.z > .9){
warp_19=`      ret.z = 0;
warp_20=`    }
warp_21=`}
comp_1=`shader_body
comp_2=`{
comp_3=`  ret = tex2D(sampler_main,uv).xyz;
comp_4=`  float3 blur1 = GetBlur2(uv);
comp_5=`  ret *= (.6+.3*sin(uv.x*10+time));
comp_6=`  blur1 = lum(blur1);
comp_7=`  float3 ret2 = tex2D(sampler_main,uv).xyz;
comp_8=`  ret = lerp(blur1,ret,pow(ret,.05+(mid_att)*.03));
comp_9=`  
comp_10=`   float2 uv1 = uv-.5;
comp_11=`   float3 ret1 = tex2D(sampler_main, uv) + GetBlur1(uv)*0;
comp_12=`    
comp_13=`   float2 hor = float2 (.001,0);
comp_14=`   float2 ver = float2 (0,.001);
comp_15=`   float dx = GetPixel (uv-hor) - GetPixel (uv+hor);
comp_16=`   float dy = GetPixel (uv-ver) - GetPixel (uv+ver);    
comp_17=`   float2 dz = float2 (dx,dy);
comp_18=`
comp_19=`   uv1 = uv1*0+.5*cos(uv1*1.5+1.6) - 3*dz;
comp_20=`   float dots = saturate(.01/length(uv1));
comp_21=`
comp_22=`   ret1 = 0.3*lum(ret1)+dots*ret*(4+(bass+mid)+treb_att);
comp_23=`   ret = ret1;    
comp_24=`}
