per_frame_4=warp = 0;
per_frame_5=wave_mystery = wave_mysteyr + 0.12 + 0.12*sin(75*time);
per_frame_6=rot = rot + 0.01*zoom;
per_frame_7=thresh = above(bass_att,thresh)*2+(1-above(bass_att,thresh))*((thresh-1.3)*0.96+1.3);
per_frame_8=dx_r = equal(thresh,2)*0.015*sin(5*time)+(1-equal(thresh,2))*dx_r;
per_frame_9=dy_r = equal(thresh,2)*0.015*sin(6*time)+(1-equal(thresh,2))*dy_r;
per_frame_10=dx = dx + 0.3*dx_r;
per_frame_11=dy = dy +0.3* dy_r;
per_frame_12=q20=(bass+treb+mid)*.3;
per_frame_13=q26=q20;
per_frame_14=q3=bass*.03;
per_frame_15=q4=treb*.03;
per_frame_16=q1=mid*.03;
per_frame_17=q2=treb-mid;
per_pixel_1=zoom = zoom + 0.01*above(rad,0.75*sin(1.75*treb));
per_pixel_2=zoom = zoom - 0.01*below(rad,0.75*sin(1.8*bass));
per_pixel_3=rot = rot + 1.3*sin(time)*if(below(rad,0.5*sin(time)),+0.2*sin(1.5*time),0);
warp_1=`shader_body
warp_2=`{
warp_3=`
warp_4=`    // sample previous frame
warp_5=`    ret.xyz = tex2D( sampler_main, uv ).xyz;
warp_6=`
warp_7=`      // take the difference between the crisp and blurred images,
warp_8=`      // then add it back into the image.  Creates spots and stripes over time.
warp_9=`      ret.xyz += (ret - GetBlur3(uv))*0.35;
warp_10=`      ret.xyz -= 0.004;
warp_11=`
warp_12=`      // add noise:
warp_13=`      float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw * 0.4 + rand_frame.xy;
warp_14=`      ret.xyz += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)*0.18;
warp_15=`
warp_16=`      // desaturate over time, to keep the globs white
warp_17=`      ret.xyz = lerp(ret.xyz, lum(ret.xyz), 0.2);
warp_18=`}
comp_1=`shader_body
