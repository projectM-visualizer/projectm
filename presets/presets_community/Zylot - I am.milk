comp_1=`shader_body
comp_2=`{
comp_3=`  ret = tex2D(sampler_main,uv).xyz;
comp_4=`  float3 colc = lum(ret - GetBlur2(uv)*.1)*2;
comp_5=`  float3 colc2 = lum(ret - GetBlur2(uv)*.1)*2;
comp_6=`  colc = pow(colc*(ret*10),(ret*colc)*50); 
comp_7=`  ret *= lerp((colc2)-colc*5,(colc)*colc,ret)+GetBlur3(uv)*30;
comp_8=`  ret = float3(abs((rad*10*q2)-1)*ret.x,abs((rad*10*q1)-1)*ret.y,abs((rad*10*q3)-1)*ret.z);
comp_9=`  ret = ret - abs(ret-1);
comp_10=`}
