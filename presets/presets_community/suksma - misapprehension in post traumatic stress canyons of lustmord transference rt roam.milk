per_frame_1=warp=0;
per_frame_2=bass_changed=abs(bass_changed-above(bass_att,1.2)*1);
per_frame_3=treb_changed=abs(treb_changed-above(treb_att,1.2)*1);
per_frame_4=mid_changed=abs(mid_changed-above(mid_att,1.2)*1);
per_frame_5=state=1+bass_changed*2+treb_changed;
per_frame_6=wave_r=if(equal(abs(state-3),1),0+.2*sin(time*.6),if(above(state,2),.6+.2*sin(time),.6+.08*sin(time)));
per_frame_7=wave_g=if(equal(abs(state-3),1),0,if(equal(state,3),0,.19));
per_frame_8=wave_b=if(equal(abs(state-3),1),.8+.2*sin(time*.27),0);
per_frame_9=ob_r=wave_g;
per_frame_10=ob_g=wave_b;
per_frame_11=ob_b=wave_r;
per_frame_12=ob_a=.3*(4-state);
per_frame_13=ob_size=.005*(4-state);
per_frame_14=pulse=if(above(abs(pulse),9.4),-9.4,pulse+.1*bor(bor(bass_changed*bnot(treb_changed),treb_changed*bnot(bass_changed))*bnot(mid_changed),mid_changed)+(mid+bass+treb)/3);
per_frame_15=q2=sin(time*state*.1);
per_frame_16=q3=cos(time*state*.1);
per_frame_17=q1=(1+1*bass_changed*above(q3,0))*(1+2*treb_changed*below(q3,0))*(1+4*mid_changed*above(q3,0))*(1+6*mid_changed*below(q3,0))*(1+10*treb_changed*above(q3,0))*(1+12*bass_changed*below(q3,0));
per_frame_18=q4=time*q1*.3*q1;
per_frame_19=q5=sin(pulse);
per_frame_20=rot=treb_changed*sin(pulse%q1*.01)*-.2;
per_frame_21=zoom=zoom+.1*bass_changed-.1*treb_changed;
per_pixel_1=studiomusic_factor=sin(ang*q1+time*.2*q1)*sin(rad*10*sin(time)-10*sin(time*.5))*sin(time*.27*q1);
per_pixel_2=pre_zoom=zoom-.1*acos(y*2-1)*sin(time*1.2*q5)-.7*cos(x*2-1)*sin(time*1.1*q5);
per_pixel_3=zoom=if(above(q3,0),pre_zoom-studiomusic_factor*.2,zoom-.05*sin(rad*.02*q1));
per_pixel_4=rot=if(above(abs(zoom-1),.03),-.4+.2*q1,rot)*if(above(q2,0),-1,1);
per_pixel_5=rot = if(below(bass,1),rot-sin(time*0.88)*.3*rad,rot);
per_pixel_6=sx=if(above(treb,.7),sx+(zoom-1)*q2,sx);
per_pixel_7=sy=if(above(treb,.7),sy+(zoom-1)*q3,sy);
warp_1=`#define sampler_pic sampler_prayerwheel
warp_2=`sampler2D sampler_pic;
warp_3=`
warp_4=`shader_body
warp_5=`{
warp_6=`    ret = tex2D( sampler_main, uv ).xyz;
warp_7=`
warp_8=`    // ERROR DIFFUSION DITHER - looks great
warp_9=`    float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw + rand_frame.xy;
warp_10=`    ret += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)/256.0*1;
warp_11=`
warp_12=`    // mix in img...
warp_13=`    float3 pic = tex2D(sampler_pic, uv_orig).xyz;
warp_14=`    float lum = lum(pic);
warp_15=`    float use_it = abs(lum - frac((bass+mid+treb)*0.2));
warp_16=`    use_it = saturate(1 - use_it*33);
warp_17=`    ret = lerp(ret, pic, use_it.xxx);
warp_18=`
warp_19=`    // darken over time
warp_20=`    //ret -= 0.004;
warp_21=`    //ret *= 0.99;
warp_22=`    ret *= 0.97 + 0.03*saturate( length(uv - uv_orig)*200 );  // center only
warp_23=`}
