per_pixel_1=thresh = above(bass_att,thresh)*2+(1-above(bass_att,thresh))*((thresh-1.3)*0.96+1.3);
per_pixel_2=dx_r = equal(thresh,2)*0.015*sin(5*time)+(1-equal(thresh,2))*dx_r;
per_pixel_3=dy_r = equal(thresh,2)*0.015*sin(6*time)+(1-equal(thresh,2))*dy_r;
per_pixel_4=
per_pixel_5=xbar = above(sin(time),0)*abs(4-x*tan(4*cos(y*time))) +
per_pixel_6=below(sin(time),0)*sin(3*x*sin(3*cos(dx_r*y*time)));
per_pixel_7=ybar = above(cos(time),0)*abs(4-y*tan(4*cos(x*time))) +
per_pixel_8=below(cos(time),0)*sin(3*y*sin(3*cos(dy_r*x*time)));
per_pixel_9=
per_pixel_10=dx = dx + dx_r*cos(6*xbar);
per_pixel_11=dy = dy + dy_r*cos(6*ybar);
per_pixel_12=zoom = zoom - 0.02*sin(3*(ybar));
per_pixel_13=rot = rot + 0.02*cos(2*(xbar));
per_pixel_14=zoom = zoom + dx_r*abs(3*rad*sin(0.5-rad));
per_pixel_15=dx = dx + dx_r*abs(0.5*sin(time)-rad);
per_pixel_16=dy = dy + dy_r*abs(0.5*sin(time)-rad);
