per_frame_1=warp = 0;
per_frame_2=wave_a = 0;
per_frame_3=tic = min(time-tin,.1);
per_frame_4=tin = time;
per_frame_5=
per_frame_6=ra = 1/tic*.25;
per_frame_7=
per_frame_8=treb_avg = tic*(treb_avg*(1/tic - ra) + ra*treb);
per_frame_9=mid_avg = tic*(mid_avg*(1/tic - ra) + ra*mid);
per_frame_10=bass_avg = tic*(bass_avg*(1/tic - ra) + ra*bass);
per_frame_11=
per_frame_12=tt = tt + tic*treb;
per_frame_13=mt = mt + tic*mid;
per_frame_14=bt = bt + tic*bass;
per_frame_15=
per_frame_16=q1 = tt*4;
per_frame_17=q2 = mt*4;
per_frame_18=q3 = bt*4;
per_frame_19=
per_frame_20=st = .1;
per_frame_21=base = .15;
per_frame_22=q4 = base + treb_avg*st;
per_frame_23=q5 = base + mid_avg*st;
per_frame_24=q6 = base + bass_avg*st;
per_frame_25=
per_frame_26=
per_frame_27=// the möbius transformation
per_frame_28=// z -> (az+b)/(cz-d); where a,b,c,d are complex numbers and z will be the uv-vector
per_frame_29=// (az+b)/(cz-d) = a/c + mu/(cz+d), with mu = (bc-ad)/c
per_frame_30=// so a/c and mu can be calculated outside of the shader
per_frame_31=
per_frame_32=//before inversion
per_frame_33=
per_frame_34=scale = 1;
per_frame_35=angle = time*.2;
per_frame_36=translation_x = 0;
per_frame_37=translation_y = 0.12;
per_frame_38=
per_frame_39=a_r = cos(angle)*scale;
per_frame_40=a_i = sin(angle)*scale;
per_frame_41=b_r = translation_x;
per_frame_42=b_i = translation_y;
per_frame_43=
per_frame_44=//complex inverted
per_frame_45=scale = 1;
per_frame_46=angle = sin(time*0.1337)*0.3;
per_frame_47=translation_u = 0;
per_frame_48=translation_v = -0.2;
per_frame_49=
per_frame_50=// c
per_frame_51=q15 = cos(angle)*scale;
per_frame_52=q16 = sin(angle)*scale;
per_frame_53=
per_frame_54=// d
per_frame_55=q17 = translation_u;
per_frame_56=q18 = translation_v;
per_frame_57=
per_frame_58=// c^(-1)
per_frame_59=c_inv_r = q15/(q15*q15+q16*q16);
per_frame_60=c_inv_i = q16/(q15*q15+q16*q16);
per_frame_61=
per_frame_62=// a*c^(-1)
per_frame_63=q11 = (a_r*c_inv_r - a_i*c_inv_i);
per_frame_64=q12 = (a_r*c_inv_i - a_i*c_inv_r);
per_frame_65=
per_frame_66=// (bc-ad)
per_frame_67=bcad_r = (b_r*q15 - b_i*q16)-(a_r*q17-a_i*q18);
per_frame_68=bcad_i = (b_r*q16 - b_i*q15)-(a_r*q18-a_i*q17);
per_frame_69=
per_frame_70=// mu*c^(-1)
per_frame_71=q13 = bcad_r*c_inv_r - bcad_i*c_inv_i;
per_frame_72=q14 = bcad_r*c_inv_i - bcad_i*c_inv_r;
per_pixel_1=ti = time*.3;
per_pixel_2=
per_pixel_3=mx = x - .5 + .25*(sin(ti*1.1278) + sin(ti*.7458));
per_pixel_4=my = y - .5 + .25*(sin(ti*.9454) + sin(ti*1.2378));
per_pixel_5=zm = cos(pow(mx*mx*1.5 + my*my,.5)*16 - q1)*q4;
per_pixel_6=dx = zm*(x-mx);
per_pixel_7=dy = zm*(y-my);
per_pixel_8=
per_pixel_9=mx = x - .5 + .25*(sin(ti*1.3488) + sin(ti*.8552));
per_pixel_10=my = y - .5 + .25*(sin(ti*.7366) + sin(ti*1.1447));
per_pixel_11=zm = cos(pow(mx*mx*1.5 + my*my,.5)*16 - q2)*q5;
per_pixel_12=dx = dx + zm*mx;
per_pixel_13=dy = dy + zm*my;
per_pixel_14=
per_pixel_15=mx = x - .5 + .25*(sin(ti*1.3544) + sin(ti*.8926));
per_pixel_16=my = y - .5 + .25*(sin(ti*.9021) + sin(ti*1.2468));
per_pixel_17=zm = cos(pow(mx*mx*1.5 + my*my,.5)*16 - q3)*q6;
per_pixel_18=dx = dx + zm*mx;
per_pixel_19=dy = dy + zm*my;
per_pixel_20=
