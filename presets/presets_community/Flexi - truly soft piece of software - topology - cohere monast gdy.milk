per_frame_40=vol = vol*vol;
per_frame_41=q1 = vol;
per_pixel_1=dy = 0.004 + 0.0005*sin(10*x+0.459*time) + 0.0005*sin(14*x+0.325*time) + 0.0005*sin(1.231*time);
