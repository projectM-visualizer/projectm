warp_1=`shader_body
warp_2=`{
warp_3=`// sample previous frame
warp_4=`ret = tex2D( sampler_main, uv);
warp_5=`    
warp_6=`float2 uvm=uv+time;
warp_7=`
warp_8=`// darken the color channels at different rates
warp_9=`ret *= float3(.9,.9,9.0)+.15*tex2d(sampler_noise_hq, uv); 
warp_10=`ret-=.002;
warp_11=`}
comp_1=`shader_body
comp_2=`{
comp_3=`//initial textures:
comp_4=`//1. Main image
comp_5=`     float3 base=tex2D(sampler_main, uv);
comp_6=`//2. High quality noise sampler
comp_7=`     float3 hinoise=tex2D(sampler_noise_hq, uv);
comp_8=`
comp_9=`//declaring modifying vars:
comp_10=`//1. Perspective UV value
comp_11=`     float2 uvp=uv;
comp_12=`     uvp.y*=.38;
comp_13=`//2. Main grid texture UV value
comp_14=`     float2 uvbm = uv;
comp_15=`     uvbm.x*=tex2d(sampler_noise_hq, uv.x-.5);
comp_16=`     uvbm.y*=tex2d(sampler_noise_hq, uv.y-.5);
comp_17=`     float2 uvpm = uvp;
comp_18=`     uvpm.x*=tex2d(sampler_noise_hq, uvp.x-.5+q2);
comp_19=`     uvpm.y*=tex2d(sampler_noise_hq, uvp.y-.5+q1);
comp_20=`
comp_21=`//3. Blur values
comp_22=`     float3 blpi=GetBlur1(uvp);
comp_23=`     float3 blpii=GetBlur2(uvp);
comp_24=`     float3 blpiii= GetBlur3(uvp);
comp_25=`     float3 blmi=GetBlur1(uv);
comp_26=`     float3 blmii=GetBlur2(uv);
comp_27=`     float3 blmiii=GetBlur3(uv);
comp_28=`
comp_29=`//Perspective modified samplers
comp_30=`//1. Perspective Main
comp_31=`     float3 pret=tex2D(sampler_main, uvp);
comp_32=`
comp_33=`//final return values
comp_34=`float3 tret=tex2d(sampler_main, uvpm);
comp_35=`float3 fret=(.17*blpiii/(pret*blpiii)).x;
comp_36=`fret-=blmii*1.35;
comp_37=`
comp_38=`ret=fret*tret*(1-tret*tret);  
comp_39=`ret*=1-.5*fret*(1-4*ret);
comp_40=`ret=1-ret;
comp_41=`ret*=ret*lum(ret-blpii);
comp_42=`ret=1-ret;
comp_43=`}
