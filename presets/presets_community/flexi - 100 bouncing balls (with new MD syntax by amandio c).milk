per_frame_1=g = 0.006/fps;    d = 0.99;    rad= .06;    
per_frame_2=
per_frame_3=i1=0;         loop(n, 
per_frame_4=
per_frame_5=x1=gmegabuf(4*i1);  y1=gmegabuf(4*i1+1);  sx1=gmegabuf(4*i1+2);  sy1=gmegabuf(4*i1+3);
