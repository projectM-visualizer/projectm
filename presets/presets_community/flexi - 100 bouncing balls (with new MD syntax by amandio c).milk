per_frame_7=x1  = x1 +  sx1;                         y1  = y1 + sy1;                       
per_frame_8=sx1 = if(below(x1,0), abs(sx1)*d,sx1);   sy1 = sy1 - g;
per_frame_9=sx1 = if(above(x1,1),-abs(sx1)*d,sx1);   sy1 = if(below(y1,0), abs(sy1)*d, sy1);
per_frame_10=
per_frame_11=i2=i1+1;      loop(n-i1-1, 
per_frame_12=
per_frame_13=x2=gmegabuf(4*i2);  y2=gmegabuf(4*i2+1);  sx2=gmegabuf(4*i2+2);  sy2=gmegabuf(4*i2+3);
per_frame_14=
per_frame_15=d1=sqrt(sqr(x1-x2)+sqr(y1-y2));
per_frame_16=d2=x1-x2+sx1-sx2;
per_frame_17=d3=y1-y2+sy1-sy2;
per_frame_18=
per_frame_19=hit = below(d1,rad) * above(d1, d2 + sqr(d3) );
per_frame_20=
per_frame_21=ang=atan2(x2-x1, y2-y1) + asin(1);
per_frame_22=s1 = sqrt(sx1*sx1 + sy1*sy1);
per_frame_23=s2 = sqrt(sx2*sx2 + sy2*sy2);
per_frame_24=w1 = atan2(sx1, sy1);
per_frame_25=w2 = atan2(sx2, sy2);
per_frame_26=
per_frame_27=sx1= if( hit, sin(ang)*s1*cos(w1-ang) + cos(ang)*s2*sin(w2-ang), sx1);
per_frame_28=sy1= if( hit, cos(ang)*s1*cos(w1-ang) - sin(ang)*s2*sin(w2-ang), sy1);
per_frame_29=sx2= if( hit, sin(ang)*s2*cos(w2-ang) + cos(ang)*s1*sin(w1-ang), sx2);
per_frame_30=sy2= if( hit, cos(ang)*s2*cos(w2-ang) - sin(ang)*s1*sin(w1-ang), sy2);
per_frame_31=
per_frame_32=
per_frame_33=gmegabuf(4*i2)=x2;   gmegabuf(4*i2+1)=y2;   gmegabuf(4*i2+2)=sx2;   gmegabuf(4*i2+3)=sy2;  
per_frame_34=i2=i2+1; ); 
