comp_2=` float2 d = texsize.zw*1;
comp_3=` float3 col=GetPixel(uv);
comp_4=`// ret=max(max(col.x,col.y),col.z); // combine layers to reduce flashing
comp_5=`ret=col;
comp_6=`float4 noise9 = tex3D(sampler_noisevol_hq, 
comp_7=`                      ((uv.xyy*q28
