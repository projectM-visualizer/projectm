warp_1=`sampler sampler_rand00;
warp_2=`sampler sampler_rand01;
warp_3=` shader_body{
warp_4=`  float2 d=texsize.zw*1;
warp_5=`  float3 blur=GetBlur1(uv);
warp_6=`  float3 sum=
warp_7=`   (
warp_8=`    tex2D(sampler_pw_main,uv.yx+d*float2(0,0))
warp_9=`    +tex2D(sampler_pw_main,uv+d*float2(-1,0))
warp_10=`    +tex2D(sampler_pw_main,uv.yx+d*float2(1,0))
warp_11=`    +tex2D(sampler_pw_main,uv+d*float2(0,-1))
warp_12=`   +tex2D(sampler_pw_main,uv+d*float2(0,-2))
warp_13=`  )*42; // the meaning of life, the universe, everything
warp_14=` ret.x=(sum.x/blur.z*roam_sin.x*.031+.01)*.06;
warp_15=` if(ret.x>18)ret.x-=17.99;
warp_16=` ret.y=(tex2D(sampler_rand00,uv).x)*.5; // preparation for combining layers, below
warp_17=` ret.z=(tex2D(sampler_rand01,uv.yx).y)*.4; //    "  "
warp_18=`float4 noise9 = tex3D(sampler_noisevol_hq, 
warp_19=`                      ((uv.xyy*q27
warp_20=`                       )*texsize.xyy*texsize_noisevol_hq.zww
warp_21=`                      ).xyz*
warp_22=`                      float3(1,1,0)*0.05 + 
warp_23=`                      time*float3(0,0,1)*q29
warp_24=`                     );
warp_25=`if(ret.x> q21*roam_cos.y && ret.x<= q24*roam_sin.x)ret.y -= (noise9).x*.5; 
warp_26=`if(ret.y> q22*roam_sin.x && ret.y<= q25*roam_cos.x)ret.z -= (noise9).y*.5; 
warp_27=`if(ret.z> q23*roam_cos.x && ret.z<= q26*roam_sin.y)ret.x -= (noise9).z*.5; 
warp_28=`}
