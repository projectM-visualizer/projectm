warp_3=`    // sample previous frame
warp_4=`    float2 uv2 = uv;
warp_5=`    uv2.x = uv.x + (.003*sin((ang*10)+(q1*5)));
warp_6=`    uv2.y = uv.y + (.003*cos((ang*10)+(q1*5)));
warp_7=`    ret = tex2D( sampler_fw_main, uv2 ).xyz;
