comp_10=`
comp_11=`shader_body
comp_12=`{
comp_13=`    float2 d = texsize.zw*8;
comp_14=`    float1 dx = ( GetBlur1(uv.yx+float2(1,0)*d)-GetBlur1(uv-float2(1,0)*d) ).y;
comp_15=`    float1 dy = ( GetBlur1(uv.yx+float2(0,1)*d)-GetBlur1(uv-float2(0,1)*d) ).y;
comp_16=`    float2 uv_y = uv+float2(dy,dx)*0.55;  
comp_17=`    float2 uv_y2 = (uv_y*1.25)-.125;  
comp_18=`
comp_19=`  
comp_20=`    ret = lerp(ret,1-ret,.7*GetBlur1(uv_y)+.2*GetPixel(uv.yx));
comp_21=`    ret = lerp(ret,ret-.1*GetBlur3(uv_y)+.1*GetBlur1(uv_y2)+.42*GetPixel(uv_y),.25);
comp_22=`    //ret *= .75;
comp_23=`    ret *= saturate(length(ret-0.5)*5);
comp_24=`    //ret = ret.xy;
comp_25=`    ret = lum(ret);
comp_26=`ret-=roam_sin.zyx*roam_cos.ywx;
comp_27=`    ret += 1.25;
comp_28=`    ret *= ret*.45;
comp_29=`    float3 mask = GetBlur3(uv)-GetBlur1(uv)-.25*GetBlur2(uv_y);
comp_30=`    ret *= lerp(ret,1.3*ret*mask,pow(hue_shader.zxy,ret));
comp_31=`    ret *=1;
comp_32=`
comp_33=`    
comp_34=`    float3 a_varaible = 0; 
comp_35=`    for(int i=0;i<5;i++)
comp_36=`      a_varaible += samples[i].w*tex2D(sampler_smalltiled_ensign_meat,ret*0.1+
comp_37=`       float2(samples[i].x*(1/texsize.x),
comp_38=`              samples[i].y*(1/texsize.y)))*0.2;
comp_39=`
comp_40=`    ret = (a_varaible);
comp_41=`    ret = 1 - (ret   + GetBlur3(ret)*1);
comp_42=`
comp_43=`}
