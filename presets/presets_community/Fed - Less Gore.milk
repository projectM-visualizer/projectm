per_frame_1=rot = rot + 0.40*( 0.60*sin(0.381*time) + 0.40*sin(0.579*time) );
per_frame_2=cx = cx + 0.410*( 0.60*sin(0.374*time) + 0.40*sin(0.294*time) );
per_frame_3=cy = cy + 0.410*( 0.60*sin(0.393*time) + 0.40*sin(0.223*time) );
per_frame_4=dx = dx + 0.03*( 0.60*sin(0.234*time) + 0.40*sin(0.277*time) );
per_frame_5=dy = dy + 0.03*( 0.60*sin(0.284*time) + 0.40*sin(0.247*time) );
per_frame_6=decay = decay - 0.01*equal(frame%6,0);
per_frame_7=dx = dx + dx_residual;
per_frame_8=dy = dy + dy_residual;
per_frame_9=bass_thresh = above(bass_att,bass_thresh)*2 + (1-above(bass_att,bass_thresh))*((bass_thresh-1.3)*0.96+1.3);
per_frame_10=dx_residual = equal(bass_thresh,2.13)*0.016*sin(time*7) + (1-equal(bass_thresh,2.13))*dx_residual;
per_frame_11=dy_residual = equal(bass_thresh,2.13)*0.012*sin(time*9) + (1-equal(bass_thresh,2.13))*dy_residual;
per_frame_12=wave_x = wave_x - dx_residual*7;
per_frame_13=wave_y = wave_y - dy_residual*7;
per_frame_14=//shader stuff
per_frame_15=q1=pow(10,zoom);
warp_1=`shader_body
warp_2=`{
warp_3=`//we do everything for the next pixel.
warp_4=`float2 r_uv_orig=uv_orig;
warp_5=`float2 r_uv=uv;
warp_6=`uv_orig=float2(uv_orig.x+0.005,uv_orig.y);
warp_7=`uv=float2(uv.x+0.005,uv.y);
warp_8=`ret=GetPixel(uv_orig);
warp_9=`//1st stage
warp_10=`float n_size = 0.005;
warp_11=`int exist_count=0;
warp_12=`exist_count += (lum(GetPixel(uv+n_size*float2( 1, 1)))>=0.7);
warp_13=`exist_count += (lum(GetPixel(uv+n_size*float2( 1, 0)))>=0.7);
warp_14=`exist_count += (lum(GetPixel(uv+n_size*float2( 1,-1)))>=0.7);
warp_15=`exist_count += (lum(GetPixel(uv+n_size*float2( 0, 1)))>=0.7);
warp_16=`exist_count += (lum(GetPixel(uv+n_size*float2( 0,-1)))>=0.7);
warp_17=`exist_count += (lum(GetPixel(uv+n_size*float2(-1, 1)))>=0.7);
warp_18=`exist_count += (lum(GetPixel(uv+n_size*float2(-1, 0)))>=0.7);
warp_19=`exist_count += (lum(GetPixel(uv+n_size*float2(-1,-1)))>=0.7);
warp_20=`ret.z+=ret.x*0.1;
warp_21=`float3 noise=-0.5*tex2D(sampler_noise_lq,uv*rand_frame.xy);
warp_22=`if(ret.z>=0.89)
warp_23=`{ret.z=0;
warp_24=`ret.x+=noise*1.5;
warp_25=`};
warp_26=`ret+=exist_count;
warp_27=`ret.z=ret.z*0.9-0.06;
warp_28=`//2nd stage
warp_29=`ret.y=abs(uv-uv_orig);
warp_30=`ret.x+=noise*0.05;
warp_31=`}
comp_1=`shader_body
comp_2=`{
comp_3=`ret=GetPixel(uv);
comp_4=`float3 orig=ret;
comp_5=`ret=GetPixel(uv+ret.xz*0.1);
comp_6=`orig.y-=0.5;
comp_7=`orig=(GetPixel(uv)+
comp_8=`GetPixel(uv+0.5*orig.y)+
comp_9=`GetPixel(uv+orig.y)+
comp_10=`GetPixel(uv+1.5*orig.y)+
comp_11=`GetPixel(uv+2*orig.y)+
comp_12=`GetPixel(uv+2.5*orig.y)+
comp_13=`GetPixel(uv+3*orig.y))/7;
comp_14=`orig.y=0;
comp_15=`ret.y=0;
comp_16=`ret=ret+orig;
comp_17=`}
