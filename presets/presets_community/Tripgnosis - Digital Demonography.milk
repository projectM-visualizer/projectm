warp_14=`    float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw + rand_frame.xy;
warp_15=`    ret += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)/256.0*float3(3,5,8);
warp_16=`
warp_17=`    // darken over time
warp_18=`    ret -= 0.0005;
warp_19=`    ret *= 0.99;
