per_frame_8=bass_eff = 0.2*max(max(bass,bass_att)-1,0);
per_frame_9=bass_thresh = above(bass_att,bass_thresh)*2 + (1-above(bass_att,bass_thresh))*((bass_thresh-1.3)*0.96+1.3);
per_frame_10=dx = 0.005 + 0.002*( 0.60*sin(0.234*time) + 0.40*sin(0.277*time) );
per_frame_11=dy = 0.005 + 0.002*( 0.60*sin(0.234*time) + 0.40*sin(0.277*time) );
per_frame_12=dx_r=if(equal(q3,0),if(above(x,xpos),dx*q1-xpos,dx+q2-xpos),dx);
per_frame_13=dy_r=if(equal(q3,0),if(above(y,ypos),dy*q1-ypos,dy+q2-ypos),dy);
per_frame_14=rot = rot+0.5*( 0.60*sin(0.381*time) + 0.40*sin(0.479*time) );
per_frame_15=mytime=.9;
per_frame_16=xpos=.5/vol;
per_frame_17=ypos=.5/vol;
per_frame_18=q1=sin(time*mytime*4);
per_frame_19=q2=cos(time*mytime*2);
per_frame_20=q3=abs(rad+.5)+(q2*q5);
per_frame_21=q4=abs(.9*5)*(dx_r*dy_r)*(dx*dy);
per_frame_22=q5=cos(.2*2)*(dx_r*dy_r);
per_frame_23=q6=0.1*(atan(abs(-rad+.5)))*q1;
per_frame_24=q7=q1+q2+q3+q4+q5+q6;
per_frame_25=ib_r=q3+q2;
per_frame_26=ib_b=q2+q1;
per_frame_27=ib_g=q1+q3;
per_frame_28=monitor=q3;
per_frame_29=zoom = zoom + 0.00025*time;
