comp_15=`    //ret *= .75;
comp_16=`    ret *= saturate(length(ret-0.5)*5);
comp_17=`    //ret = ret.xy;
comp_18=`    ret = lum(ret);
comp_19=`    ret += 1.25;
comp_20=`    ret *= ret*.45;
comp_21=`    float3 mask = GetBlur3(uv)-GetBlur1(uv)-.25*GetBlur2(uv_y);
comp_22=`    ret *= lerp(ret,1.3*ret*mask,pow(hue_shader.zxy,ret));
comp_23=`    ret += .03;
comp_24=`    ret *= ret;
comp_25=`    ret *= hue_shader.zxy;
