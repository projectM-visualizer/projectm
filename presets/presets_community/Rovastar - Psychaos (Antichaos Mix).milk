per_frame_16=volume = 0.15*(bass_att+bass+mid+mid_att);
per_frame_17=beatrate = if(equal(beatrate,0),1,if(below(volume,0.01),1,beatrate));
per_frame_18=lastbeat = if(equal(lastbeat,0),time,lastbeat);
per_frame_19=meanbass_att = 0.1*(meanbass_att*9 + bass_att);
