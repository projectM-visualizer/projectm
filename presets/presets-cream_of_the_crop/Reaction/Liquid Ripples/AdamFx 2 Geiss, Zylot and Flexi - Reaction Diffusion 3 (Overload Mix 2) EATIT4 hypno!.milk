warp_20=`      float2 pix = texsize.zw*12;
warp_21=`      float3 dx = (GetBlur2(uv_orig + float2(1,0)*pix) - GetBlur2(uv_orig - float2(1,0)*pix));
warp_22=`      float3 dy = (GetBlur2(uv_orig + float2(0,1)*pix) - GetBlur2(uv_orig - float2(0,1)*pix));
warp_23=`      float2 my_uv = uv_orig + float2(dx.x,dy.x)*0.005 + float2(dx.y,dy.y)*0.005 - float2(dx.z,dy.z)*0.005;
warp_24=`
