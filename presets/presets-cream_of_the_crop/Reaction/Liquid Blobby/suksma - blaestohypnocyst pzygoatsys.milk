per_frame_36=mv_a=.2+.2*sin(time*1.178+q5*1.14);
per_frame_37=rot=0;
per_frame_38=wave_b = cos(time)  + abs(cos(time));
per_frame_39=wave_g = abs(sin(time)) ;
