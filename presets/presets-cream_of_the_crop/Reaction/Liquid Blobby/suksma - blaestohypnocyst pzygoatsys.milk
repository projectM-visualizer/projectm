per_frame_29=ib_g = abs(sin(time)) ;
per_frame_30=ib_r = (-1 * cos(time))  + abs(-1 * cos(time)) + 0.2 * (cos(sin(time))+(abs(cos(sin(time)))+cos(sin(time))));
per_frame_31=ib_size=.05+.03*q2;
