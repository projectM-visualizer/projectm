per_frame_1=warp = 0;
per_frame_2=old_bass_flop=bass_flop;
per_frame_3=old_treb_flop=treb_flop;
per_frame_4=old_mid_flop=mid_flop;
per_frame_5=chaos=.9+.1*sin(pulse);
per_frame_6=bass_thresh = above(bass_att,bass_thresh)*2 + (1-above(bass_att,bass_thresh))*((bass_thresh-1.3)*chaos+1.3);
per_frame_7=bass_flop=abs(bass_flop-equal(bass_thresh,2));
per_frame_8=treb_thresh=above(treb_att,treb_thresh)*2 + (1-above(treb_att,treb_thresh))*((treb_thresh-1.3)*chaos+1.3);
per_frame_9=treb_flop=abs(treb_flop-equal(treb_thresh,2));
per_frame_10=mid_thresh=above(mid_att,mid_thresh)*2 + (1-above(mid_att,mid_thresh))*((mid_thresh-1.3)*chaos+1.3);
per_frame_11=mid_flop=abs(mid_flop-equal(mid_thresh,2));
per_frame_12=bass_changed=bnot(equal(old_bass_flop,bass_flop));
per_frame_13=mid_changed=bnot(equal(old_mid_flop,mid_flop));
per_frame_14=treb_changed=bnot(equal(old_treb_flop,treb_flop));
per_frame_15=bass_residual = bass_changed*sin(time*bass_thresh) + bnot(bass_changed)*bass_residual;
per_frame_16=treb_residual = treb_changed*sin(time*treb_thresh) + bnot(treb_changed)*treb_residual;
per_frame_17=mid_residual = mid_changed*sin(time*mid_thresh) + bnot(mid_changed)*mid_residual;
per_frame_18=vol=max(max(mid,bass),treb)*.03;
per_frame_19=pulse=if(above(abs(pulse),5000),-5000,pulse+vol*2*bor(bor(bass_changed,treb_changed),mid_changed)+vol);
per_frame_20=q1=mid_residual;
per_frame_21=q2=bass_residual;
per_frame_22=q3=treb_residual;
per_frame_23=q4=sin(pulse);
per_frame_24=q5=cos(pulse);
per_frame_25=wave_r=wave_r+.5*bass_residual;
per_frame_26=wave_r=wave_g+.5*mid_residual;
per_frame_27=wave_r=wave_b+.5*treb_residual;
per_frame_28=zoom=zoom+.03*q5;
per_frame_29=rot=q4*.03;
per_pixel_1=anti_rad=1-rad;
per_pixel_2=yank_x=(x*2-rad)*10;
per_pixel_3=spank_x=(x*2-anti_rad)*10;
per_pixel_4=yank_y=(y*2-rad)*10;
per_pixel_5=spank_y=(x*2-anti_rad)*10;
per_pixel_6=yank_rad=(rad*2+x)*10;
per_pixel_7=spank_rad=(rad*2+y)*10;
per_pixel_8=whip_me=cos(yank_x*q1-spank_x*q4+yank_y*q2-spank_y*q5+yank_rad*q3-spank_rad*sin(time))*.1;
per_pixel_9=zoom=zoom-whip_me;
warp_1=`shader_body
warp_2=`{
warp_3=`
warp_4=`float2 uv1 = uv_orig + (uv-uv_orig)*1.2;
warp_5=`float2 uv2 = uv_orig + (uv-uv_orig)*0.6;
warp_6=`float2 uv3 = uv_orig + (uv-uv_orig)*1;
warp_7=`
warp_8=`    // sample previous frame
warp_9=`    ret.x = tex2D( sampler_main, uv1 ).x;
warp_10=`    ret.y = tex2D( sampler_main, uv2 ).y;
warp_11=`    ret.z = tex2D( sampler_main, uv3 ).z;
warp_12=`
warp_13=`    float3 b;
warp_14=`    b.x = lerp(GetBlur2(uv1),GetBlur1(uv1),0.5).x;
warp_15=`    b.y = lerp(GetBlur2(uv2),GetBlur1(uv2),0.5).y;
warp_16=`    b.z = lerp(GetBlur2(uv3),GetBlur1(uv3),0.5).z;
warp_17=`
warp_18=`      ret += (ret - b)*0.3;
warp_19=`      ret *= 0.95;
warp_20=`      ret += 0.04;
warp_21=`}
comp_1=`
comp_2=`
comp_3=`
comp_4=`
comp_5=`shader_body
comp_6=`{
comp_7=`    uv = 0.05 + 0.9*uv;
comp_8=`    ret = tex2D(sampler_main, uv).xyz;
comp_9=`      // SUPER GLOW EDGES - looks awesome w/octopus
comp_10=`      float3 avg_col = GetBlur3(uv);
comp_11=`      ret = abs(avg_col - ret)*6;
comp_12=`    ret *= 1.333; // a little bit of overbright
comp_13=`ret.x=1-ret.x;
comp_14=`ret.y=1-ret.y;
comp_15=`ret.z=1-ret.z;
comp_16=`}
