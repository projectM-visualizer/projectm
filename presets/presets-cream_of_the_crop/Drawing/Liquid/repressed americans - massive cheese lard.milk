per_frame_40=//flexi mobius spiral col frame
per_frame_41=t = t + 2.5/fps;
per_frame_42=t = if(above(t,2),t-2,t);
per_frame_43=db = db*0.98 + bass_att*0.2; // dampener
per_frame_44=bb = bb + db*0.1; // accumulation, replace time expressions with this ;)
per_frame_45=dt = dt*0.98 + treb_att*0.2;
per_frame_46=tt = tt + dt*0.1;
per_frame_47=dm = dm*0.98 + mid_att*0.2;
per_frame_48=mm = mm + dm*0.1;
per_frame_49=w = (bb-tt)*0.1;                   // angle
per_frame_50=q32 = 0.8 - (db-dt)*0.04;         // size
per_frame_51=q31 = sin(w);
per_frame_52=q30 = cos(w);
per_frame_53=q29 = sin(-w);
per_frame_54=q28 = cos(-w);
per_frame_55=q27 = 1/q32;
