per_frame_init_1=p1 = 2000; p2 = 200; p3=50;
per_frame_1=dec_med = pow (0.9, 30/fps);
per_frame_2=dec_slow = pow (0.9, 30/fps);
per_frame_3=beat = max (max (bass, mid), treb); 
per_frame_4=avg = avg*dec_slow + beat*(1-dec_slow);
per_frame_5=is_beat = above(beat, .1+avg+peak) * above (time, t0+.2);
per_frame_6=t0 = is_beat*time + (1-is_beat)*t0;
per_frame_7=peak = is_beat * beat + (1-is_beat)*peak*dec_med;
per_frame_8=index = (index + is_beat) %16;
per_frame_9=index2 = (index2 + is_beat*bnot(index))%5;
per_frame_10=monitor = index2;
per_frame_11=q1 = is_beat;
per_frame_12=q3 = bass + mid + treb;
per_frame_13=q4 = index2;
per_frame_14=k1 =  is_beat*equal(index%2,0);
per_frame_15=p1 =  k1*(p1+1) + (1-k1)*p1;
per_frame_16=p2 = dec_med * p2+ (1-dec_med)*p1;
per_frame_17=rott_ = p2 * 3.1416/4;
per_frame_18=rott =  rott +  .2*30/fps*q2;
per_frame_19=q2 = sin(rott);
per_frame_20=ran = bnot(q1)*ran + q1 * (int(rand(50))-24);
per_frame_21=tran = tan(ran);
per_frame_22=tran = max(tran,-5);
per_frame_23=tran = min(tran,5);
per_frame_24=trel = trel + .02*30/fps*tran+q1;
per_frame_25=q5 = cos(trel);
per_frame_26=q6 = sin(trel);
per_frame_27=q7 = -q6;//it makes no sense, but commenting q7/8 fucks it up majorly
per_frame_28=q8 = q5;
per_frame_29=movex = movex*dec_slow + .1*sin(rott)*30/fps;
per_frame_30=spd = spd*bnot(q1) + q3*q1;
per_frame_31=movez = movez + .005*30/fps*spd;
per_frame_32=q9 = movez;
per_frame_33=mv_x = 20;
per_frame_34=mv_y = 15;
per_frame_35=mv_a = .1;
