per_frame_1=vol = bass*8 + mid*5 + treb*3;
per_frame_2=m = m*0.97 + vol*0.08;
per_frame_3=monitor = vol;
per_frame_4=beat = above(vol,res)*above(vol,m)*above(vol,16);
per_frame_5=diff = (1-beat)*diff + beat*(vol-res);
per_frame_6=res = beat*(vol + m*0.1) + (1-beat)*(res -  (0.1+diff*0.02)*60/fps);
per_frame_7=res = max(0,res);
per_frame_8=
per_frame_9=
per_frame_10=x = if(beat,rand(60)*0.01 + 0.2,x);
