per_frame_34=b2x = b2x + b2vx*fScale;      b2y = b2y + b2vy*fScale;
per_frame_35=
per_frame_36=// o Collision and "keep-in-screen" check
per_frame_37=q1 = if( below(sqr(b1x-.5) +sqr(b1y-.5), .005), 1, 0);
per_frame_38=q1 = if( above( frame, q7) ,q1, 0 );
